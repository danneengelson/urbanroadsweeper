caption always

bindkey ^w screen
bindkey ^p prev
bindkey ^n next
bindkey ^x quit
# To quit with Ctrl+A+q
bind q quit
bindkey ^| windowlist

screen 1
title "Garage - BA*"
stuff "sh run.sh garage bastar\015"
stuff "sh show.sh garage bastar\015"

screen 2
title "Garage - Inward Spiral"
stuff "sh run.sh garage spiral\015"
stuff "sh show.sh garage spiral\015"


screen 3
title "Garage - Sampled BA* & Inward Spiral"
stuff "sh run.sh garage sampled\015"
stuff "sh show.sh garage sampled\015"

screen 4
title "Bridge - BA*"
stuff "sh run.sh bridge bastar\015"
stuff "sh show.sh bridge bastar\015"


